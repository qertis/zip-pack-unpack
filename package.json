{
  "name": "zip-pack-unpack",
  "version": "0.0.1",
  "description": "Simple zip and unzip",
  "main": "index.cjs",
  "module": "index.mjs",
  "scripts": {
    "build": "babel src/index.mjs -d dist --out-file-extension .cjs --minified",
    "test": "node tests/test.mjs"
  },
  "dependencies": {
    "archiver": "~5.3.1",
    "yauzl": "~2.10.0"
  },
  "files": [
    "dist",
    "index.mjs"
  ],
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "keywords": [
    "zip",
    "unzip",
    "pack",
    "unpack",
    "archiver"
  ],
  "engines": {
    "node": ">=18.x",
    "npm": ">=8.x"
  },
  "author": "Denis Baskovsky (https://baskovsky.ru)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "~7.19.3",
    "@babel/core": "~7.20.2",
    "@babel/preset-env": "~7.20.2"
  }
}
